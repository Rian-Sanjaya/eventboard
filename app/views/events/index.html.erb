<h1>All Event</h1>

<div>
	<%= link_to "Add Event", new_event_path %>
</div>

<% @events.each do |event| %>
	<div>
		<h4><%= link_to event.title, event_path(event) %></h4>
		<p><%= time_format(event.start_date) %></p>
		<p><%= time_format(event.end_date) %></p>
		<p><%= event.venue %>, <%= event.location %> <%= fa_icon "camera-retro" %></p>
	</div>
<% end %>